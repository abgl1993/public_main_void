public class Array {
	/**
	 * 
	 * @param args
	 */
	public static void main(String args[]){
	
		/*Process p=new Process();
		int[] result=p.create();
	    p.display(result);*/	
		int[][][] num;
		num=new int[5][3][2];
		num[0][1][1]=5;
		
		for(int[][] dim1:num){
			for(int[] dim2:dim1){
				for(int value:dim2){
					System.out.print(value);
				}
			}
			//System.out.println();
		}
	}
	
}
