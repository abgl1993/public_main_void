public class Array {
	/**
	 * 
	 * @param args
	 */
	public static void main(String args[]){
	
		/*Process p=new Process();
		int[] result=p.create();
	    p.display(result);*/	
		int[][] num;
		num=new int[5][3];
		num[1][1]=5;
		
		for(int[] data:num){
			for(int src:data){
				System.out.println(src);
			}
			System.out.println();
		}
	}
	
}
